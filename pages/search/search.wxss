.header {
  width: 100%;
  height: 104rpx;
  border-bottom: 1rpx solid #F0F0F0;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
  background: #fff;
}
.header-left{
  width: 85%;
  height: 104rpx;
  float: left;
  padding-left: 26rpx;
  box-sizing: border-box;
}
.header-left view{
  width: 100%;
  height: 60rpx;
  position: relative;
  background: #F0F0F0;
  border-radius: 40rpx;
  top: 21rpx;
}
.search-input{
  height: 60rpx;
  text-align: left;
  line-height: 60rpx;
  font-size: 24rpx;
  color: #A9A9A9;
  box-sizing: border-box;
  padding-left: 70rpx;
}
.search-icon{
  width: 24rpx;
  height: 24rpx;
  position: absolute;
  top: 18rpx;
  left: 36rpx;
  z-index: 10;
}
.header-right{
  width: 15%;
  height: 104rpx;
  float: left;
  font-size: 28rpx;
  color: #A3A0A0;
  text-align: center;
  line-height: 104rpx;
}
.content{
  /* height: 104rpx; */
  margin-top:124rpx;
}
.noresult{
  display: block;
  text-align: center;
  margin-top: 500rpx;
}