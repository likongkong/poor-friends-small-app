.bg {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: -1;
  opacity: 0.2;
}

.head {
  width: 710rpx;
  height: 120rpx;
  background: #fff;
  border: 8rpx solid #000;
  border-radius: 24rpx;
  margin: 30rpx auto 0 auto;
  display: flex;
  /* justify-content: space-between; */
  align-items: center;
  padding: 0 20rpx;
  box-sizing: border-box;
}

.head-name {
  font-size: 36rpx;
  color: #000;
  font-weight: bold;
  margin-right: 120rpx;
}

.head view {
  display: flex;
  align-items: center;
}

.head view:last-child {
  margin-right: 0;
}

.head-volleyball {
  margin-right: 36rpx;
}

.head image {
  width: 64rpx;
  height: auto;
}

.head view text {
  font-size: 24rpx;
  color: #505050;
  margin-left: 12rpx;
}

.content {
  width: 694rpx;
  position: fixed;
  top: 176rpx;
  bottom: 226rpx;
  left: 20rpx;
  background: #fff;
  border: 8rpx solid #000;
  border-radius: 24rpx;
  text-align: center;
  overflow: hidden;
}

.title {
  font-size: 36rpx;
  font-weight: bold;
  color: #000;
  text-align: center;
  display: block;
  width: 100%;
  margin-top: 20rpx;
  margin-bottom: 28rpx;
}

.award {
}

/* .award view:first-child {
  margin-right: 40rpx;
} */

.award view {
  min-width: 114rpx;
  height: 114rpx;
  background: rgba(255, 221, 29, 0.62);
  border: 6rpx solid #ffdd1d;
  border-radius: 8rpx;
  display: inline-block;
  position: relative;
  margin: 0 25rpx;
}

.award image {
  width: 64rpx;
  margin-top: 25rpx;
}

.award text {
  width: 100%;
  display: block;
  font-size: 20rpx;
  color: #06365f;
  position: absolute;
  text-align: center;
  bottom: -40rpx;
}

.signBtn {
  min-width: 240rpx;
  padding: 0 60rpx;
  display: inline-block;
  height: 92rpx;
  background-image: linear-gradient(-180deg, rgba(255, 3, 3, 0.61) 0%, rgba(255, 14, 14, 0.61) 100%);
  border: 4rpx solid #000;
  border-radius: 53rpx;
  margin-top: 60rpx;
  font-size: 36rpx;
  color: #fff;
  line-height: 84rpx;
}

.awardDetail {
  position: absolute;
  bottom: 0;
  left: 0;
}

.awardDetail>view:first-child {
  margin-bottom: 4rpx;
}

.signBox {
  width: 100%;
  height: 144rpx;
  background: rgba(255, 221, 29, 0.62);
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.signBox>view:first-child {
  width: 170rpx;
  height: 144rpx;
  border-right: 4rpx solid #fff;
}

.signBox>view:last-child {
  width: 520rpx;
  height: 144rpx;
}

.week {
  bottom: 144rpx;
}

.signBox-l .txt {
  font-size: 24rpx;
  color: #505050;
  display: block;
  margin: 16rpx 0 18rpx 0;
}

.signBox-l view {
  font-size: 26rpx;
}

.signBox-l view text {
  font-size: 48rpx;
  margin-right: 5rpx;
}

.signBox-r .txt {
  font-size: 24rpx;
  color: #505050;
  display: block;
  margin: 16rpx 0 20rpx 0;
  text-align: left;
  padding-left: 36rpx;
}

.signBox-r>view {
  padding-left: 36rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

.signBox-r>view>view {
  display: flex;
  align-items: center;
  margin-right: 40rpx;
}

.signBox-r>view>view image {
  width: 56rpx;
  height: auto;
  margin-right: 16rpx;
}

.signBox-r>view>view text {
  font-size: 24rpx;
  color: #505050;
}

.weekNum {
  color: #ff834b;
}

.month {
  border-bottom-left-radius: 20rpx;
  border-bottom-right-radius: 20rpx;
}

.monthNum {
  color: #6e6e6e;
}

.footer {
  width: 710rpx;
  display: flex;
  justify-content: space-between;
  position: fixed;
  bottom: 20rpx;
  left: 20rpx;
}

.footer>view {
  width: 200rpx;
  height: 164rpx;
  background: #fff;
  border: 8rpx solid #000;
  border-radius: 24rpx;
}

.footer>view image {
  margin: 20rpx auto 20rpx auto;
  display: block;
}

.explain image {
  width: 60rpx;
  height: auto;
}

.recommend {
  position: relative;
}

.recommend image {
  width: 66rpx;
  height: auto;
}

.recommend button {
  position: absolute;
  top: 0;
  left: 0;
  opacity: 0;
  width: 100%;
  height: 100%;
}

.shangcheng image {
  width: 72rpx;
  height: auto;
}

.footer>view text {
  font-size: 28rpx;
  color: #000;
  display: block;
  text-align: center;
}

.sign_mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.2);
  overflow: hidden;
}

.sign_succeed {
  width: 536rpx;
  height: 380rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  background: #fff;
  z-index: 10;
  margin-left: -268rpx;
  margin-top: -165rpx;
  border-radius: 20rpx;
}

.sign_succeed-t {
  width: 100%;
  height: 284rpx;
  background: url('data:image/png;base64,')no-repeat;
  background-size: 100% 100%;
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
  /* border-bottom: 4rpx solid #ffe400; */
  padding-top: 42rpx;
  box-sizing: border-box;
}

.sign_succeed-t text {
  display: block;
  text-align: center;
}

.sign_succeed-t>text:first-child {
  font-size: 40rpx;
  font-weight: bold;
  margin: 0 0 10rpx 0;
}

.sign_succeed-t>text:nth-child(2) {
  font-size: 28rpx;
}

.sign_gift {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 30rpx;
}

.sign_gift>view {
  margin: 0 20rpx;
}

.sign_gift image {
  width: 54rpx;
  margin: auto;
  display: block;
}

.sign_gift text {
  font-size: 20rpx;
  color: #06365f;
  margin-top: 10rpx;
}

.sign_succeed-b {
  width: 100%;
  height: 92rpx;
  font-size: 36rpx;
  color: #818384;
  text-align: center;
  line-height: 92rpx;
}

