.wrap{
  width: 100%;
}
.head{
  width: 100%;
  height: 230rpx;
  padding: 34rpx 28rpx 0 28rpx;
  box-sizing: border-box;
}
.portrait{
 display: inline-block;
 float: left;
}
.portrait view:last-child{
  margin-right: 0;
}
.portrait-box{
  width: 196rpx;
  height: 196rpx;
  background: #e3e3e3;
  margin-right: 49rpx;
  float: left;
}
.add-portrait{
  width: 192rpx;
  height: 192rpx;
  border: 4rpx solid #e3e3e3;
  float: left;
  margin-left:49rpx;
}
.content{
  width: 100%;
}
.module{
  width: 694rpx;
  height: 88rpx;
  border-bottom: 2rpx solid #e7e7e7;
  margin-left: 26rpx;
  position: relative;
}
.module input{
  width: 694rpx;
  height: 88rpx;
  box-sizing: border-box;
  font-size: 28rpx;
  color: #323232;
}
.agreement{
  font-size: 24rpx;
  text-align: center;
  padding-top: 52rpx;
  box-sizing: border-box;
}
.agreement text{
  font-size: 24rpx;
  color: #0074ff;
}
.btn{
  width: 360rpx;
  height: 76rpx;
  background: #FC688C;
  border-radius: 76rpx;
  font-size: 40rpx;
  color: #fff;
  text-align: center;
  line-height: 76rpx;
  margin: 52rpx auto 52rpx auto;
}