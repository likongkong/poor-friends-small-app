.city{
  width: 144rpx;
  height: 76rpx;
  background: #fff;
  border-radius: 4rpx;
}
.city text{
  width: 100%;
  height: 76rpx;
  display: block;
  line-height: 76rpx;
  padding: 0 30rpx;
  color: #b1b1b1;
  font-size: 24rpx;
  text-align: center;
  box-sizing: border-box;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
}
.select{
  position: absolute;
  top: 34.5rpx;
  right: 20rpx;
  width: 16rpx;
  /* height: 7rpx; */
}
.loading{
  position: absolute;
  top: 18rpx;
  left: 50%;
  margin-left: -20rpx;
  width: 40rpx;
  height: 40rpx;
}
.head-search{
  width: 100%;
  height: 136rpx;
  background: #FFDD1D;
  padding: 40rpx;
  box-sizing: border-box;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.search{
  width: 486rpx;
  height: 76rpx;
  background: #fff;  
  border-radius: 4rpx;
  float: right;
  padding-left: 69rpx;
  position: relative;
  box-sizing: border-box;
}
.search-input{
  height: 76rpx;
  text-align: left;
  font-size: 24rpx;
  color: #A9A9A9;
  box-sizing: border-box;
}
.search-icon{
  width: 24rpx;
  height: 24rpx;
  position: absolute;
  top: 26rpx;
  left: 35rpx;
  z-index: 10;
}
.search-placeholder{
  color: #A9A9A9;
  font-weight: 100;
}

.content{
  width: 100%;
  position: relative;
  top: 136rpx;
  left: 0;
  z-index: 1;
}
.nav{
  width: 100%;
  height: 392rpx;
  padding: 34rpx;
  box-sizing: border-box;
}
.nav>view{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.nav_menu{
  display: inline-block;
}
.nav_menu image{
  width: 100rpx;  
}
.nav_menu text{
  display: block;
  font-size: 24rpx;
  color: #4E4D4A;
}

.banner{
  width: 100%;
  height: 234rpx;
  background: #f6f6f6;
  display: flex;
  align-items: center;
}
