.carousel{
  width: 100%;
  background-color: rebeccapurple;
}
.carousel_swiper{
  width: 100%;
  height: 234rpx;
  display: block;
  position: relative;
  background: #f4f4f4;
}
.swiper-item{
  position: relative;
}
.carousel_swiper .swiper_mask{
  position: absolute;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 10;
  background-color: rgba(0,0,0,0.2);
}
.swiperTest{
  position: absolute;
  z-index: 100;
}
.swiper-label{
  width: 146rpx;
  height: 40rpx;
  display: block;
  background: #F2E5F5;
  font-size: 18rpx;
  color: #9B25AF;
  border-radius: 6rpx;
  text-align: center;
  line-height: 40rpx;
  top: 20rpx;
  left: 34rpx;
}
.swiper-title{
  font-size: 30rpx;
  color: #FFFFFF;
  left: 34rpx;
  bottom: 92rpx;
}
.swiper-txt{
  font-size: 24rpx;
  color: #FFFFFF;
  left: 34rpx;
  bottom: 50rpx;
}
.carouselImg{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: inherit;
  display: block;
}

.nav_tab{
  width: 100%;
  height: 118rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.nav_tab>view{
  height: 118rpx;
  line-height: 118rpx;
  position: relative;
}
.nav_tab>view:first-child{
  margin-right: 114rpx;
}
.nav_tab>view text{
  font-size: 32rpx;
  color: #100D03;
  font-weight: 200;
  display: block;
}

.tab_active text{
  font-weight: bold !important;
}
.tab_active image{
  width: 120rpx;
  height: auto;
  position: absolute; 
  bottom: 20rpx;
}


.rec_tab{
  margin-left: 34rpx;
  margin-bottom: 30rpx;
}
.pet_recommend{
  width: 100%;
  /* padding: 0 34rpx;
  box-sizing: border-box; */
}
.rec_active{
  background: #FFDD1D;
  border: 1px solid #FFDD1D !important;
  color: #100D03 !important; 
  font-weight: bold !important;
}
.rec_tab text{
  display: inline-block;
  font-size: 24rpx;
  color: #C8C6BB;
  border: 1px solid #E3E1D5;
  border-radius: 4rpx;
  padding: 6rpx 24rpx;
  margin-right: 20rpx;
}



.c_list_b{
  margin-top: 20rpx;
  padding: 30rpx 40rpx;
  background: #fff;
  box-sizing: border-box;
}
.c_dan_list{
  width: 100%;
  overflow: hidden;
}
.c_dan_list>view{
  float: left;
}
.c_dan_list_l{
  width: 431rpx;
  height: 180rpx;
  padding-right: 35rpx;
  box-sizing: border-box;
  position: relative;
}
.c_dan_list_l>text{
  display: block;
  font-size: 32rpx;
  color: #000000;
  font-weight: bold;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.c_dan_list_l .author_info{
  width: 100%;
  overflow: hidden;
  position: absolute;
  bottom: 0;
  left: 0;
}
.c_dan_list_l .author_info image{
  width: 40rpx;
  height: 40rpx;
  /* border-radius: 52rpx; */
  display: inline-block;
  float: left;
  margin-top: 2rpx;
}
.c_dan_list_l .author_info .name{
  float: left;
  width: 182rpx;
  display: inline-block;
  font-size: 24rpx;
  color: #999;
  margin-left: 5rpx;
  height: 52rpx;
  line-height: 52rpx;
  overflow:hidden;
    text-overflow:ellipsis;
    white-space:nowrap;
}
.c_dan_list_l .author_info .time{
  height: 52rpx;
  font-size: 24rpx;
  line-height: 52rpx;
  color: #000;
  opacity: .3;
  float: right;
  display: inline-block;
  padding-right: 40rpx;
  box-sizing: border-box;
}
.c_dan_list_r{
  width: 240rpx;
  /* background: #FFDD1D */
}
.c_dan_list_r image{
  width: 100%;
  height: 180rpx;
  display: block;
  border-radius: 8rpx;
}