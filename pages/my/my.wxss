}

.chunk{
  width: 100%;
  height: 128rpx;
  background: #fff;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding: 0 0 0 40rpx;
  box-sizing: border-box;
  position: relative;
}
.chunk image{
  width: 48rpx;
  height: auto;
}
.chunk .text{
  width:100%;
  height: 128rpx;
  display: block;
  line-height: 128rpx;
  font-size: 32rpx;
  color: #1F1D1D;
  margin-left: 40rpx;
  position: relative;
}
.chunk button{
  width: 100%;
  height: 128rpx;
  line-height: 128rpx;
  font-size: 32rpx;
  color: #1F1D1D;
  background: transparent;
  text-align: left
}
.chunk .news{
  width: 18rpx;
  height: 18rpx;
  background: #FF4040;
  border-radius: 18rpx;
  position: absolute;
  top: 24rpx;
  right: 22rpx;
}

