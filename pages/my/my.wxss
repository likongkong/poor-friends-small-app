  border-radius: 120rpx;
  overflow: hidden;
}
.head-b .portrait view image{
  width: 100%;
  height: auto;
}
.head-b .name{  
  width: 566rpx;
  padding-right: 132rpx;
  box-sizing: border-box;
  position: relative;
