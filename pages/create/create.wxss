  color: #636363;
  text-align: center;
  line-height: 52rpx;
  /* margin-right: 60rpx; */
}
.label_box text:last-child{
  /* margin-right: 0; */
}
.input{
  height: 82rpx;
  border-bottom: 2rpx solid #EAEAEA;
  display: flex;
  align-items: center;
  position: relative;
}
