}

.deletePic{
  font-size: 20rpx;
  padding: 20rpx 40rpx;
  box-sizing: border-box;
  color: red
}

.portrait-box {
  width: 196rpx;
  height: 196rpx;
  background: #e3e3e3;
  margin-right: 39rpx;
  float: left;
  margin-bottom: 10rpx;
}

.portrait-box image {
  width: 196rpx;
  height: 196rpx;
}

.add-portrait {
  width: 192rpx;
  height: 192rpx;
  border: 2rpx dashed #e3e3e3;
  float: left;
  text-align: center;
}

.add-portrait image {
  width: 72rpx;
  height: 72rpx;
  margin-top: 60rpx;
}

.content {
  width: 100%;
  padding-bottom: 52rpx;
  padding-top: 28rpx;
  box-sizing: border-box;
}
