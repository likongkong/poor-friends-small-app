.disclaimer {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1000;
}

.disclaimer-mask {
  width: 100%;
  height: 100%;
  background: #000;
  opacity: 0.24;
  position: fixed;
  top: 0;
  left: 0;
}

.disclaimer-box {
  width: 520rpx;
  height: 400rpx;
  background: #fff;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  border-radius: 8rpx;
  overflow: hidden;
}

.disclaimer-box view {
  width: 100%;
  height: 100rpx;
  position: absolute;
  left: 0;
  bottom: 0;
  background: #ffc926;
  font-size: 40rpx;
  color: #fff;
  text-align: center;
  line-height: 100rpx;
  font-weight: 200;
}

.disclaimer-box text {
  display: block;
  text-align: center;
  font-size: 28rpx;
}

.disclaimer-box .title {
  font-weight: bold;
  color: #000;
  margin: 27rpx 0 40rpx 0;
  font-size: 32rpx;
}



.swiper{
  width: 100%;
  height: 100%;
}
.scroll_view{
  position: absolute;
  top: 0;
  bottom: 52rpx;
}
.form{
  width: 100%;
  display: block;
  padding: 0 40rpx;
  box-sizing: border-box;
}
.title-h1{
  width: 100%;
  font-size: 36rpx;
  color: #000000;
  font-weight: bold;
  margin-top: 40rpx;
