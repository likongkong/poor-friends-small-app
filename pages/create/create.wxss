.input input{
  width: 100%;
  font-size: 24rpx;
  color: #999;
  font-weight: 200;
}
.textarea_box{
  position: relative;
}
.textarea{
  width: 100%;
  height: 160rpx;
  border: 2rpx solid #F4F4F4;
  border-radius: 8rpx;
  font-size: 24rpx;
  color: #999;
  box-sizing: border-box;
  margin-top: 30rpx;
}
.textareaMask{
  width: 100%;
  height: 160rpx;
  border: 2rpx solid #F4F4F4;
  border-radius: 8rpx;
  font-size: 24rpx;
  color: #d5d5d5;
  padding: 14rpx 14rpx;
  box-sizing: border-box;
  margin-top: 30rpx;
}
.actuality_label{
  overflow: hidden;
}
.actuality_label view{
  display: inline-block;
  margin-right: 8rpx;
  float: left;
  margin-top: 16rpx;
}
.actuality_label text{
  height: 52rpx;
  padding: 0 22rpx;
  box-sizing: border-box;
  font-size: 24rpx;
  color: #636363;
  border: 2rpx solid #F4F4F4;
  border-radius: 26rpx;
  display: inline-block;
  line-height: 52rpx;
  /* margin-top: 30rpx; */
  margin-right: 26rpx;
}
.address .input image{
  width: 20rpx;
  height: 30rpx;
  position: absolute;
  right: 0;
  bottom: 26rpx;
}
.address .input::before{
  display: none
}
.address input{
  padding-right: 60rpx;
  box-sizing: border-box;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  word-break: normal
}
.btn_box{
  display: flex;
  justify-content: space-between
}
.btn_box button{
  width: 300rpx;
}

