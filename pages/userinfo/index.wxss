.head{width: 100%;height: 200rpx;border: 1px solid #EEEEEE;}
.head image{width: 120rpx; height: 120rpx; border-radius: 120rpx;overflow: hidden;float: left;margin: 40rpx 40rpx 0 26rpx;}
.head text{display: block;font-size: 32rpx;color: #323232;line-height: 200rpx;}

.infobox{width: 90%;margin-left: 5%;border-bottom: 1px solid #EEEEEE;padding-top: 28rpx;}
.infobox text{display: block;}
.infobox text:nth-child(1){font-size: 28rpx;color: #A2A2A2;font-weight: 200;}
.infobox text:nth-child(2){line-height: 112rpx;font-size: 32rpx;font-weight: 200;}
.foter{width: 90%;margin-left: 5%;padding: 80rpx 0 100rpx 0;overflow: hidden;text-align: center; display: flex;justify-content: center;}
.foter button{width: 320rpx;height: 76rpx;line-height: 76rpx;font-size: 36rpx;border: none;}
/* .foter button:nth-child(1){float: left;color: #fff;background: #B4B4B4;} */
.foter button:nth-child(1){color: #000;background: #FFDD1D;}