.collection-list{
  width: 100%;
  height: 150rpx;
  padding: 40rpx 26rpx;
  box-sizing: border-box;
  overflow: hidden;
  border-bottom: 2rpx solid #EBEBEB;
}
.collection-list image{
  width: 100%;
  height: 100%;
  border-radius: 100%;
}
.collection-list-left{
  width: 70%;
  float: left;
  height: 76rpx;
}
.collection-list-left view{
  width: 76rpx;
  height: 76rpx;
  border-radius: 76rpx;
  background: #333;
  float: left;
  margin-right: 20rpx;
}
.collection-list-left text{
  float: left;
  display: block;
  height: 76rpx;
  line-height: 76rpx;
  font-size: 24rpx;
  color: #575656 ;
}
.collection-list-right{
  width: 20%;
  float: right;
  height: 76rpx;
  line-height: 76rpx;
  color: #9C9C9C;
  font-size: 20rpx;
  text-align: right;
}
.no-data{
  display: block;
  text-align: center;
  margin-top: 500rpx;
  font-size: 32rpx;
  color: #9C9C9C;
}