/**app.wxss**/
.load{
  width: 100%;
  height: 100%;
  background: #fff;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 100;
}
button::after{
  border: none
}
.mask{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 100;
  background: #fff;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.hide{
  display: none !important;
}
.show{
  display: block !important;
}
.marginT12{
  margin-top: 10rpx;
}
.card{
  width: 670rpx;
  margin-left: 40rpx;
  box-shadow: 1px 1px 10rpx 0 rgba(192,192,192,0.50);
  border-radius: 10rpx;
  padding: 32rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  margin-bottom: 40rpx;
}
