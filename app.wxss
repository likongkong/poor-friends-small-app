.card-left{
  width: 254rpx;
  height: 226rpx;
  border-radius: 10rpx;
  overflow: hidden;
}
.card-left image{
  width: 254rpx;
  height: auto;
}
.card-photo{
  width: 200rpx;
  margin-left: 30rpx;
  height: 140rpx;
}
.card-photo image{
  width: 100%;
  height: 140rpx;
}
.card-right{
  width: 344rpx;
  padding-left: 24rpx;
  box-sizing: border-box;
}
.card-right>text{
  width: 90%;
  display: block;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
}
.card-title{
  font-size: 32rpx;
  color: #323232;
  font-weight: bold;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  width: 100%;
  display: block;
}
.card-age{
  font-size: 24rpx;
  color: #969696;
  font-weight: 200;
  display: block;
}
.card-status{
  width: 116rpx;
  height: auto;
}
.card-name{
  display: flex;
  justify-content: flex-start;
  align-items: center;
  font-weight: 200;
}
.card-name>text{
  display: inline-block;
  font-size: 24rpx;
  color: #969696;
  margin-right: 10rpx;
}
.card-name>image{
  width: 24rpx;
  height: auto;
}
.card-txt-icon{
  width: 35rpx;
  height: 35rpx;
  float: left;
  margin-right: 6rpx;
}

.card-address{
  width: 100%;
  height: 20rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.card-address image{
  width: 20rpx;
  height: 24rpx;
  margin-right: 10rpx;
}
.card-address text{
  width: 100%;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  height: 24rpx;
  line-height: 24rpx;
  font-size: 24rpx;
  color: #969696;
  display: block;
  font-weight: 200;
}

.no_sy{
  text-align: center;
}
.no_sy image{
  width: 220rpx;
  height: 192rpx;
  margin-top: 200rpx;  
}
.no_sy text{
  display: block;
  width: 100%;
  text-align: center;
  margin-top: 5rpx;
  font-size: 28rpx;
  color: #ABABAB;
}

.login::after{
  border: none;
}

/* 授权登录框样式 */
.login{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background-color:rgba(0,0,0,0.5);
  z-index: 10;
}
.login>view{
  width: 558rpx;
  height: 434rpx;
  background: #fff;
  border-radius: 10rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top: -217rpx;
  margin-left: -279rpx;
}
.gb-img{
  width: 80rpx;
  height: 80rpx;
  position: absolute;
  top: 0;
  right: 0;
  display: block;
}
.gb-img image{
  width: 26rpx;
  height: 27rpx;
  display: block;
  margin-top: 27.5rpx;
  margin-left: 26.5rpx;
}
.login-title{
  width: 100%;
  display: block;
  font-size: 36rpx;
  color: #333333;
  text-align: center;
  margin-top: 70rpx;
  margin-bottom: 39rpx;
}
.login>view button{
  width: 458rpx;
  height: 90rpx;
  padding-left: 60rpx;
  box-sizing: border-box;
}
.login>view button image{
  width: 57rpx;
  float: left;
  display: block;
  margin-top: 11.75rpx;
}
.login>view button text{
  display: inline-block;
  float: left;
  font-size: 30rpx;
  line-height: 90rpx;
  margin-left: 30rpx;
}
.wx-login{
  background: #11b01d;
  margin-bottom: 30rpx;
  margin-top: 100rpx;
}
.wx-login::after{
   border: none; 
}
.wx-login text{
  color: #fff;
}
.sj-login{
  background: #f8f8f8;
}
.sj-login::after{
  border:2rpx solid #d7d7d7;
}
.sj-login text{
  color: #666666;
}
.mask{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 100;
  background: #fff;
}


.noMore{
  width: 100%;
  font-size: 28rpx;
  color: #333;
  text-align: center;
  height: 80rpx;
  /* background: #fff; */
}
.noMore view{
  display: inline-block;
  overflow: hidden;
}
.noMore image{
  float: left;
  width: 45rpx;
  height: 45rpx;
  margin-top: 15rpx;
  margin-right: 10rpx;
}
.noMore text{
  float: left;
  display: block;
  height: 80rpx;
  line-height: 80rpx;
}
